<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bongo1st"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.movies.startingPage.StartingPage"
            android:label="Bongo" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.movies.browseAll.Movies" >
        </activity>
        <activity android:name="com.movies.singleMovie.PlayMovie" >
        </activity>
        <activity android:name="com.movies.singleMovie.SingleMoviePage" >
        </activity>
        <activity android:name="com.movies.movieSummary.MovieSummary" >
        </activity>
        <activity android:name="com.user.Login" >
        </activity>
        <activity android:name="com.movies.actorProfile.ActorProfile" >
        </activity>
        <activity android:name="com.movies.singleMovie.TestActivity" >
        </activity>
        <activity android:name="com.tab.TabAndListView" >
        </activity>
        <activity android:name="com.movies.catagory.Catagory" >
        </activity>
        <activity android:name="com.movies.movieTiming.MovieTiming" >
        </activity>
    </application>

</manifest>